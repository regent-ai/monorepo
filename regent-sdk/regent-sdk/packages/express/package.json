  "dependencies": {
    "@regent/core": "workspace:*",
    "@regent/x402": "workspace:*",
    "@regent/types": "workspace:*",
    "express": "^4.19.2",
    "x402": "^0.7.1",
    "x402-express": "^0.7.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@regent/http": "workspace:*",
    "@types/express": "^4.17.21",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
