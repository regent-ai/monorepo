{
  "name": "@regent/contracts",
  "version": "0.1.0",
  "description": "TypeScript interfaces and clients for Regent agent-creation contracts",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./abi": {
      "types": "./dist/abi/index.d.ts",
      "import": "./dist/abi/index.js",
      "default": "./dist/abi/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/regent-protocol/regent-sdk",
    "directory": "packages/contracts"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "test": "bun test",
    "type-check": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "regent",
    "agent",
    "contracts",
    "ethereum",
    "viem"
  ],
  "license": "MIT",
  "dependencies": {
    "@regent/types": "workspace:*",
    "viem": "catalog:"
  },
  "devDependencies": {
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
