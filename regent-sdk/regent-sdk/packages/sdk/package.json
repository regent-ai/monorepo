{
  "name": "regent-sdk",
  "version": "0.1.0",
  "description": "Unified SDK for agent runtime, HTTP adapters, x402 payments, and ERC-8004 identity",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/regent-protocol/regent-sdk",
    "directory": "packages/sdk"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "regent",
    "agent",
    "sdk",
    "x402",
    "erc-8004",
    "blockchain"
  ],
  "license": "MIT",
  "dependencies": {
    "@regent/core": "workspace:*",
    "@regent/http": "workspace:*",
    "@regent/hono": "workspace:*",
    "@regent/wallet": "workspace:*",
    "@regent/x402": "workspace:*",
    "@regent/a2a": "workspace:*",
    "@regent/erc8004": "workspace:*",
    "@regent/types": "workspace:*"
  },
  "devDependencies": {
    "tsup": "catalog:",
    "typescript": "catalog:",
    "@regent/eslint-config": "workspace:*",
    "@regent/prettier-config": "workspace:*"
  }
}
