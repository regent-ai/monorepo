{
  "name": "@regent/cli",
  "version": "1.10.1",
  "description": "Scaffold a Bun agent app powered by @regent/core.",
  "repository": {
    "type": "git",
    "url": "https://github.com/regent-protocol/regent-sdk",
    "directory": "packages/cli"
  },
  "type": "module",
  "bin": {
    "regent": "dist/index.js",
    "create-agent-kit": "dist/index.js"
  },
  "files": [
    "dist",
    "templates",
    "adapters"
  ],
  "scripts": {
    "test": "bun test",
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc -p tsconfig.json --noEmit"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "agent",
    "scaffold",
    "bun",
    "regent",
    "erc-8004"
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^24.3.1",
    "bun-types": "^1.1.16",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "@regent/eslint-config": "workspace:*",
    "@regent/prettier-config": "workspace:*"
  }
}
